{namespace soy.page}

/**
*/
{template .repos}

{literal}
<style>
.github-widget {
    margin-top: 30px;
}

.github-widget .github-box .link {
    margin-top: 1em;
}
</style>
{/literal}

    <h1>Libraries and Tools</h1>

    {foreach $repo in ['soy-grunt-task', 'zoneinfo-server']}
        {call .repo}{param repoName : $repo /}{/call}
    {/foreach}

{/template}

/**
 * @param repoName
*/
{template .repo}
    <div class="github-widget" data-repo="hitsthings/{$repoName}"></div>
{/template}